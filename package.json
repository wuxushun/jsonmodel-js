{
  "name": "jsonmodel-js",
  "version": "0.5.0",
  "description": "JSON Model for javascript",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "umd/jsonmodel.umd.js",
  "scripts": {
    "lint": "eslint src --fix",
    "test": "jest",
    "clean": "shx rm -rf dist && shx rm -rf bin",
    "build": "rollup -c",
    "content": "shx mkdir bin && shx cp -R dist/* bin && shx cp {README.md,package.json} bin",
    "build:release": "npm run clean && npm run build && npm run content",
    "deploy": "pushd ./bin && npm publish && popd"
  },
  "keywords": [
    "JSON",
    "JSONModel",
    "model",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wuxushun/jsonmodel-js.git"
  },
  "author": "wuxushun@icloud.com",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.5.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.68.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
